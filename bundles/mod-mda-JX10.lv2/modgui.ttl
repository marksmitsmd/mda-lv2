@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://moddevices.com/plugins/mda/JX10>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-mda-jx10.html> ;
        modgui:stylesheet <modgui/stylesheet-mda-jx10.css> ;
        modgui:screenshot <modgui/screenshot-mda-jx10.png> ;
        modgui:thumbnail <modgui/thumbnail-mda-jx10.png> ;
        modgui:brand "MDA" ;
        modgui:label "JX10" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "wood1" ;
        modgui:knob "bronze" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "env_att" ;
            lv2:name "Attack" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "env_dec" ;
            lv2:name "Decay" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "env_sus" ;
            lv2:name "Sustain" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "env_rel" ;
            lv2:name "Release" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "vcf_freq" ;
            lv2:name "Filter" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "vcf_reso" ;
            lv2:name "Reson" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "vibrato" ;
            lv2:name "Vibrato" ;
        ] ;
    ] .